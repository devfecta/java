package com.devfecta;

import java.util.ArrayList;
import java.util.List;

import com.google.gson.Gson;

public class Main {
	public static void main( String[] args )
    {
    	Gson gson = new Gson();
        System.out.println(gson.toJson("Hello World!"));
        System.out.println(displayArray());
    }
    
    public static String displayArray() {
    	List<String> sampleArrayList = new ArrayList<String>();
    	sampleArrayList.add("Luke");
    	sampleArrayList.add("Han");
    	sampleArrayList.add("Lando");
    	return sampleArrayList.get(2);
    }
}
